{
  "arrowParens": "always",
  "jsxBracketSameLine": false,
  "printWidth": 120,
  "trailingComma": "es5",
  "tabWidth": 2,
  "importOrder": ["^[./]+.*/[a-zA-Z]+[\\w]*$", "^[./].*/[\\w?&]*$", "^[./].*/[\\w_\\-?]*[.][\\w._\\-?&]*$"],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,
  "importOrderCaseInsensitive": true
}

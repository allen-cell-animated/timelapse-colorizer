    "lint": "eslint --config ./.eslintrc --ext .ts --ext .tsx --ext .js --ext .jsx .",
    "lint-fix": "eslint --fix --config ./.eslintrc --ext .ts --ext .tsx --ext .js --ext .jsx .",
    "typeCheck": "tsc -p tsconfig.json --noEmit"
  },
  "devDependencies": {
